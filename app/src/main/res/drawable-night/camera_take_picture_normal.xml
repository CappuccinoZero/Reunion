<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@color/take_picture_normal" />
        </shape>
    </item>

    <item
        android:top="3dp"
        android:bottom="3dp"
        android:end="3dp"
        android:start="3dp"
        android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/take_picture_normal" />
        </shape>
    </item>
</layer-list>